    "excel4node": "^1.7.2",
    "fast-csv": "^4.3.1",
    "jquery": "^3.5.1",
    "papaparse": "^5.2.0",
    "read-excel-file": "^4.0.6",
    "shell": "^0.5.0"
  },
  "devDependencies": {
    "electron-winstaller": "^4.0.1"
  }
