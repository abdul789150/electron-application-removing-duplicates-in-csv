  "main": "index.js",
  "productName": "File Agency",
  "scripts": {
    "start": "electron .",
    "build": "npm run build-win",
    "build-mac": "electron-packager . --platform=darwin --out=mac_build --icons=assets/icon/logo.icns --overwrite",
    "build-win": "electron-packager . --platform=win32 --out=win_build --icons=assets/icon/logo.ico --overwrite --asar=true",
    "build-linux": "electron-packager . --platform=linux --out=linux_build --icons=assets/icon/logo.png --overwrite",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
